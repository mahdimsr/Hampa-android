<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape android:shape="rectangle">


            <corners
                android:radius="36dp"
                />

            <stroke
                android:color="@color/colorCheckBoxGrade"
                android:width="2dp"
                />

        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="rectangle">

            <corners
                android:radius="36dp"
                />

            <solid
                android:color="#FFFFFF"
                />

        </shape>
    </item>


</layer-list>